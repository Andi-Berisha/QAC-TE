Width: '400px', margin: '0 auto' }}>\r\n      {/* <label>Name on credit card:</label><br/> */}\r\n      {/* <input type=\"text\" name=\"name\" /> */}\r\n      <CardElement options={CARD_OPTIONS} />\r\n      <button type=\"submit\" disabled={!stripe}>\r\n        Pay\r\n      </button>\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default CheckoutForm;","import { useState } from 'react';\r\nimport { Elements } from '@stripe/react-stripe-js';\r\nimport { loadStripe } from '@stripe/stripe-js';\r\nimport CheckoutForm from './CheckoutForm';\r\nimport { Button } from 'reactstrap';\r\n// import PUBLISHABLE_KEY from './.env'\r\n// require('dotenv').config();\r\n\r\n// const stripePromise = loadStripe(process.env.PUBLISHABLE_KEY);\r\nconst stripePromise = loadStripe('pk_test_51IRJCSK9eOXpEB8jQfIX7nJGm8jrshmJJ0mISdUCUAYbjT4lV9hZLxvCfclftnED2Q7rgLz7KVeUWEDR0vhfCawY00r9aDXSb8');\r\n\r\n// console.log(process.env.PUBLISHABLE_KEY)\r\n\r\nconst PaymentModal = ({bookingID, amount}) => {\r\n    const [status, setStatus] = useState(\"ready\");\r\n\r\n    if (status === \"success\") {\r\n        return <div> <p>Payment Successfully Processed!</p>\r\n                     <Button href=\"/whatsOn\" color=\"secondary\">Back to Listings</Button></div>;\r\n    }\r\n    return (\r\n        <div className=\"container\">\r\n\r\n\r\n\r\n            <Elements stripe={stripePromise}>\r\n                <CheckoutForm success={() => {\r\n                    setStatus(\"success\");\r\n                }} bookingID={bookingID} amount ={amount}/>\r\n            </Elements>\r\n\r\n        </div>\r\n    )\r\n\r\n}\r\n\r\nexport default PaymentModal;","import { useState } from 'react';\r\nimport films from '../../Resources/films.json';\r\nimport { Button, Form, FormGroup, Label, Input, Col, Row, Modal, ModalHeader, ModalBody, ModalFooter } from 'reactstrap';\r\nimport { BOOKINGS_URL } from \"../../Resources/CONSTS.json\";\r\nimport axios from \"axios\";\r\nimport PaymentModal from './PaymentModal';\r\nconst B